[Unit]
Description=Myriadcoin
After=network.target

[Service]
ExecStart=/home/pi/myriadcoin/bin/myriadcoind -datadir=/home/pi/myriadcoin-data -debuglogfile=/home/pi/myriadcoind.log -rpcpassword=rpc -port=_PORT_ -disablewallet -listen -discover -upnp _ARGS_
StandardOutput=inherit
StandardError=inherit
Restart=always
User=pi

[Install]
WantedBy=multi-user.target
